    "clean": "rm -rf dist && rm -rf node_modules && yarn cache clean && yarn setup"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/node": "18.11.18",
    "@types/uuid": "10.0.0",
    "class-transformer": "^0.5.1",
    "ts-node": "10.9.1",
    "ttypescript": "1.5.15",
    "typescript": "4.9.5",
    "typescript-transform-paths": "3.4.6"
  },
  "dependencies": {
    "@types/validator": "^13.12.1",
    "class-validator": "0.14.1",
    "express": "4.19.2",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "uuid": "10.0.0",
    "validator": "^13.12.0"
  }
}
