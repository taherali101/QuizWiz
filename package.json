{
  "name": "QuizWiz",
  "version": "1.0.0",
  "description": "RESTful APIs for a quiz application that allows users to answer multiple-choice Questions",
  "author": "Taher Ali",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "ttsc && node dist/index.js",
